    "scripts": {
        "test": "jest --detectOpenHandles --forceExit",
        "predev": "yarn run types",
        "dev": "cd src && ts-node index.ts -e ts,graphql",
        "dev:local": "cp .env.localhost .env && yarn run dev",
        "dev:cloud": "cp .env.development .env && yarn run dev",
        "dev:prod": "cp .env.production .env && yarn run dev",
        "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
        "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
        "build": "tsc",
        "prebuild": "rm -rf dist",
        "postbuild": "yarn copy src/api/**/*.graphql src/api/**/*.html dist/api",
        "start": "cd dist && node index.js"
    },
    "dependencies": {
        "@typegoose/typegoose": "^6.2.1",
